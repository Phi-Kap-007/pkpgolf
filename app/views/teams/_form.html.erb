<div class="container">
  <div class="row">
    <div class="col-sm-4 col-sm-offset-4">
      <%= form_for @team do |f| %>
        <%= f.label :name, "Enter a Name" %>
        <%= f.text_field :name %>
        <%= f.label :tee_off_time, "Enter Tee-Off Time" %>
        <%= f.text_field :tee_off_time %>

        <%= f.label :members, "Enter Members" %>
        <%= f.text_field :members %>

        <%= f.submit %>
      <% end %>
    </div>
  </div>
</div>

