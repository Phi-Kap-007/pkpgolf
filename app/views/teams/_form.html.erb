<div class="container">
  <div class="row">
    <div class="col-sm-4 col-sm-offset-4">
      <%= form_for @team do |f| %>
        <%= f.label :name, "Enter a Name" %>
        <%= f.text_field :name %>
        <%= f.label :tee_off_time, "Enter Tee-Off Time" %>
        <%= f.text_field :tee_off_time %>
        <%= f.label :member_1 %>
        <%= f.text_field :member_1 %>
        <%= f.label :member_2 %>
        <%= f.text_field :member_2 %>
        <%= f.label :member_3 %>
        <%= f.text_field :member_3 %>
        <%= f.label :member_4 %>
        <%= f.text_field :member_4 %>
        <%= f.submit %>
      <% end %>
    </div>
  </div>
</div>

